<div class="scrollable">
  <table class="table table-hover table-bordered index-table table-condensed">
    <thead>
      <tr>
        <th class="key">key</th>
        <th class="value">value</th>
      </tr>
    </thead>
    <tbody style="height: 100px; overflow-y: scroll;">
      <%
        for(var i = 0; i < models.length; i++) {
          var model = models[i];
      %>
      <tr data-id="<%- model.get('id') %>">
        <td class="key"><%- model.get('key') %></td>
        <td class="value"><input type="text" name="value" value="<%- model.get('value') %>" data-cid="<%- model.cid %>"></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
